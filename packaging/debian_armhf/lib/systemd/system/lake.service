[Unit]
Description=openbank lake message relay
After=inetd.service

[Service]
Type=notify
EnvironmentFile=/etc/init/lake.conf
StartLimitBurst=0
ExecStart=/openbank/services/lake/lake
StandardInput=null
LimitNOFILE=1048576
LimitNPROC=infinity
LimitCORE=infinity
Restart=on-failure
RestartSec=1
TimeoutStartSec=0
KillMode=process

[Install]
WantedBy=multi-user.target
